<h1><%= t(".title") %></h1>
<%= form_with(scope: :session, url: sessions_path, local: true) do |f| %>
  <%= f.label :email, t("activerecord.attributes.user.email") %><br>
  <%= f.email_field :email, class:"form-control" %><br>
  <%= f.label :password, t("activerecord.attributes.user.password") %><br>
  <%= f.password_field :password, class:"form-control" %><br>
  <%= f.submit "ログイン", id: "create-session", class:"btn btn-primary" %>
<% end %>