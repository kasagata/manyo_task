<h1><%= t('.title') %></h1>
<% if @user.errors.any? %>
  <div class="error_explanation">
    <h2>アカウント作成に失敗しました</h2>
    <ul>
      <% @user.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
    </ul>
  </div>
<% end %>
<%= render 'form', user: @user, submit_user_id: 'create-user' %>